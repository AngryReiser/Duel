{
  "name": "duel",
  "version": "1.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Findoss/Duel.git"
  },
  "author": "Findoss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Findoss/Duel/issues"
  },
  "homepage": "https://github.com/Findoss/Duel#readme",
  "scripts": {
    "build-api": "aglio --theme-variables flatly -i docs/source/api/index.apib -o docs/build/API.html",
    "build-doc": "",
    "build-structure": "tree -I \"node_modules|.git|.vscode\" -a > docs/build/progect-structure.txt",
    "dev-api-mock": "drakov -f docs/source/api/*.apib -s docs/source/api/index.apib -p 3001 --watch  --autoOptions --method DELETE --method PATCH --header Authorization --debugMode",
    "dev-api": "aglio --theme-variables slate -i docs/source/api/index.apib --server",
    "test-api": "dredd docs/source/*.apib http://127.0.0.1:3001"
  },
  "dependencies": {},
  "devDependencies": {
    "aglio": "^2.3.0",
    "drakov": "^1.0.4",
    "dredd": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "tree-node-cli": "^1.2.2"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "import/extensions": "off",
      "import/no-unresolved": "off"
    }
  }
}
